<%- include('../../partials/header.ejs'); %>
<%- include('../../partials/homenavbar.ejs'); %>
<div class="container">
    <hr>
<% result.articles.rows.forEach(article => { %>
    <div class="card">
        <div class="card-header">
            <h2><%= article.title %></h2>
        </div>
        <div class="card-body">
            <a href="/<%= article.slug %>" class="btn btn-success">Ler Artigo</a>
        </div>
        <br>
    </div>
<% }) %>
</div>

<%- include('../../partials/footer.ejs'); %>

